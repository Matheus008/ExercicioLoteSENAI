package loteECamisetas.objeto;

public class Lote {
	
	private int numeroDoLote;
	private int quantidadeDePeças;
	private Camisetas[] camisetas;
	
	public Lote(int numeroDoLote, int quantidadeDePeças, Camisetas[] camisetas) {
		this.numeroDoLote = numeroDoLote;
		this.quantidadeDePeças = quantidadeDePeças;
		this.camisetas = new Camisetas[quantidadeDePeças];
	}
	
	
}
